{
    "name": "upwebdesign/laravel-infusionsoft",
    "description": "This package provides port of the Infusionsoft SDK utilizing Laravel fascades.",
    "keywords": ["laravel", "infusionsoft", "sdk", "isdk"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jesse Vista",
            "email": "jvista@upwebdesign.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "~6.1",
        "illuminate/console": "~5.0",
        "illuminate/support": "~5.0",
        "illuminate/routing": "~5.0",
        "infusionsoft/php-sdk": "~1.3.6",
        "league/flysystem": " ~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1"
    },
    "autoload": {
        "classmap": [
        ],
        "psr-4": {
            "Upwebdesign\\Infusionsoft\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "Upwebdesign\\Infusionsoft\\InfusionsoftServiceProvider"
            ],
            "aliases": {
                "Infusionsoft": "Upwebdesign\\Infusionsoft\\InfusionsoftFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
