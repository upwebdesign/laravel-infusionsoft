{
  "name": "upwebdesign/laravel-infusionsoft",
  "description": "This package provides port of the Infusionsoft SDK utilizing Laravel fascades.",
  "keywords": [
    "laravel",
    "infusionsoft",
    "sdk",
    "isdk"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jesse Vista",
      "email": "jvista@upwebdesign.com"
    }
  ],
  "require": {
    "php": "^7.2|^8.0",
    "illuminate/console": "5.7.*|5.8.*|^6.0|^7.0|^8.0",
    "illuminate/support": "5.7.*|5.8.*|^6.0|^7.0|^8.0",
    "illuminate/routing": "5.7.*|5.8.*|^6.0|^7.0|^8.0",
    "infusionsoft/php-sdk": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "Upwebdesign\\Infusionsoft\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "5.0-dev"
    },
    "laravel": {
      "providers": [
        "Upwebdesign\\Infusionsoft\\InfusionsoftServiceProvider"
      ],
      "aliases": {
        "Infusionsoft": "Upwebdesign\\Infusionsoft\\InfusionsoftFacade"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
